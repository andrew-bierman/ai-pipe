{
  "name": "ai-pipe",
  "version": "1.0.8",
  "description": "A lean CLI for calling LLMs from the terminal. Text in, text out.",
  "keywords": [
    "ai",
    "cli",
    "llm",
    "openai",
    "anthropic",
    "pipe",
    "terminal",
    "openrouter",
    "vercel-ai-sdk",
    "command-line-tool",
    "developer-tools",
    "generative-ai",
    "shell"
  ],
  "homepage": "https://github.com/andrew-bierman/ai-pipe#readme",
  "bugs": {
    "url": "https://github.com/andrew-bierman/ai-pipe/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrew-bierman/ai-pipe.git"
  },
  "license": "GPL-3.0-only",
  "author": "Andrew Bierman",
  "type": "module",
  "module": "src/index.ts",
  "bin": {
    "ai": "src/index.ts",
    "ai-pipe": "src/index.ts"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build src/index.ts --compile --outfile dist/ai-pipe",
    "build:all": "bun run build:mac && bun run build:mac-x64 && bun run build:linux && bun run build:linux-arm",
    "build:linux": "bun build src/index.ts --compile --target=bun-linux-x64 --outfile dist/ai-pipe-linux-x64",
    "build:linux-arm": "bun build src/index.ts --compile --target=bun-linux-arm64 --outfile dist/ai-pipe-linux-arm64",
    "build:mac": "bun build src/index.ts --compile --target=bun-darwin-arm64 --outfile dist/ai-pipe-darwin-arm64",
    "build:mac-x64": "bun build src/index.ts --compile --target=bun-darwin-x64 --outfile dist/ai-pipe-darwin-x64",
    "lint": "bunx biome check --write .",
    "lint:check": "bunx biome check .",
    "start": "bun src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^4.0.55",
    "@ai-sdk/anthropic": "^3.0.41",
    "@ai-sdk/azure": "^3.0.27",
    "@ai-sdk/cohere": "^3.0.19",
    "@ai-sdk/deepinfra": "^2.0.32",
    "@ai-sdk/deepseek": "^2.0.18",
    "@ai-sdk/fireworks": "^2.0.32",
    "@ai-sdk/google": "^3.0.23",
    "@ai-sdk/google-vertex": "^4.0.50",
    "@ai-sdk/groq": "^3.0.22",
    "@ai-sdk/huggingface": "^1.0.30",
    "@ai-sdk/mcp": "^1.0.19",
    "@ai-sdk/mistral": "^3.0.19",
    "@ai-sdk/openai": "^3.0.26",
    "@ai-sdk/perplexity": "^3.0.18",
    "@ai-sdk/togetherai": "^2.0.31",
    "@ai-sdk/xai": "^3.0.50",
    "@openrouter/ai-sdk-provider": "^2.2.1",
    "ai": "^6.0.78",
    "ai-sdk-ollama": "^3.5.0",
    "commander": "^14.0.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/bun": "latest",
    "lefthook": "^2.1.0",
    "sort-package-json": "^3.6.1"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.8",
  "engines": {
    "bun": "^1.3.8",
    "node": ">=22.0.0"
  }
}
